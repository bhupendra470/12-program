#include<stdio.h>
#include<conio.h>
void main()
{
int s1,s2,s3,s4,s5,sum;
float per;
clrscr ();
printf("enter the value of five subject=");
scanf("%d %d %d %d %d,&s1,&s2,&s3,&s4,&s5");
sum=s1+s2+s3+s4+s5;
printf("sum of five subject is%d \n",sum);
per=(sum*100)/500;
printf("percentage is%f",per);
getch();
return 0;
}
